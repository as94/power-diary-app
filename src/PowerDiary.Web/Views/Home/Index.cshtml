@model PowerDiary.Domain.Reports.Contracts.HighGranularityReportContract

@{
    ViewData["Title"] = "Chat events report";
}

<div class="text-center">
    <h1 class="display-4">Chat events report</h1>
</div>

<div class="container mt-5">
    <h1 class="mb-4">High Granularity Report</h1>
    <table class="table table-bordered">
        <thead class="thead-dark">
        <tr>
            <th>Formatted Time</th>
            <th>Message</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var entry in Model.Rows)
        {
            <tr>
                <td>@entry.FormatedTime</td>
                <td>@entry.Message</td>
            </tr>
        }
        </tbody>
    </table>
</div>